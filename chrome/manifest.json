{
	"name": "Slow for you?",
	"version": "0.1",
	"manifest_version": 2,
	"description": "Figure out if the Internet's slow for everyone in your office.",
	"permissions": ["http://localhost:3000/"],
	"content_scripts": [
		{
			"matches": ["http://*/*", "https://*/*"],
			"js": ["reporter.js"]
		}
	],
    "browser_action": {
        "default_title": "Slow4U?",
        "default_icon": "Blue.png",
        "default_popup": "ckstatus.html"
    }	
}